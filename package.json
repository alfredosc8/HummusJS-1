{
  "name": "@alfredosc8/hummus2",
  "version": "1.0.109",
  "description": "Create, read and modify PDF files and streams",
  "homepage": "http://pdfhummus.com/",
  "license": "Apache-2.0",
  "author": "Gal Kahana <gal.kahana@hotmail.com>",
  "contributors": [
    "Alfredo Scotto <alfredosc8@gmail.com>"
  ],
  "main": "./hummus.js",
  "types": "./hummus.d.ts",
  "scripts": {
    "build": "node-pre-gyp build",
    "rebuild": "node-pre-gyp rebuild",
    "install": "node-pre-gyp install --fallback-to-build $EXTRA_NODE_PRE_GYP_FLAGS",
    "test": "mocha -R tap ./tests/*.js --timeout 15000",
    "package": "node-pre-gyp package",
    "publish": "node-pre-gyp publish",
    "deploy": "npm package; npm publish; npm deploy --access public"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/alfredosc8/HummusJS-1.git"
  },
  "keywords": [
    "pdf",
    "pdfhummus"
  ],
  "files": [
    "src",
    "hummus.js",
    "binding.gyp",
    "PDFRStreamForFile.js",
    "PDFStreamForResponse.js",
    "PDFWStreamForFile.js",
    "PDFRStreamForBuffer.js",
    "PDFWStreamForBuffer.js"
  ],
  "dependencies": {
    "node-pre-gyp": "^0.14.0"
  },
  "bundledDependencies": [
    "node-pre-gyp"
  ],
  "devDependencies": {
    "aws-sdk": "^2.660.0",
    "chai": "^4.2.0",
    "mocha": "^7.1.1",
    "npm": "^6.14.4"
  },
  "binary": {
    "module_name": "hummus",
    "module_path": "./binding",
    "remote_path": "./{module_name}/v{version}/{toolset}/",
    "host": "https://asc8-hummus.s3-us-west-2.amazonaws.com",
    "package_name": "{node_abi}-{platform}-{arch}-{libc}.tar.gz"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
